\name{plot.rtconnect}
\alias{plot.rtconnect}
\title{
  Plot for rtconnect object.
}
\description{
  Plot units of rtconnect object.
}
\usage{
  \method{plot}{rtconnect}(x, type="daily", graph.type="b",
      main=NULL, xlab=NULL, ylab=NULL, ...)
}
\arguments{
  \item{x}{
    rtconnect object.
  }
  \item{type}{
    string giving the type of plot. The following values are possible:
    "daily" and "weekly".
  }
  \item{graph.type}{
    1-character string giving the type of plot desired.
  }
  \item{main}{
    overall title for the plot.
  }
  \item{xlab}{
    a label for the x axis.
  }
  \item{ylab}{
    a label for the y axis.
  }
  \item{...}{
  }
}
